{
  "name": "eric-sdk",
  "version": "0.1.6",
  "description": "Official SDK for enforcing policy-governed AI execution using the Eric AI governance layer",
  "author": "Rod Bridges",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
